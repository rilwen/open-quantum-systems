Import('env')
sources = Glob('*.cpp')
sources.extend(Glob('linalg/*.cpp'))
sources.extend(Glob('integr/*.cpp'))
sources.extend(Glob('interp/*.cpp'))
sources.extend(Glob('ode/*.cpp'))
sources.extend(Glob('opt/*.cpp'))
sources.extend(Glob('sf/*.cpp'))
sources.extend(Glob('solvers/*.cpp'))
sources.extend(Glob('stats/*.cpp'))
math = env.StaticLibrary('math', sources)
Return('math')
