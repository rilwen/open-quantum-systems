Import('env')
Import('protein_chain')
Import('math')
Import('GTEST_LIBS')
Import('OTHER_LIBS')
penv = env.Clone()
penv.Append(LIBS = [protein_chain, math] + GTEST_LIBS + OTHER_LIBS)
penv.Append(CPPPATH = '#protein_chain')
protein_chain_test = penv.Program('protein_chain-test', Glob('*.cpp'))
Return('protein_chain_test')