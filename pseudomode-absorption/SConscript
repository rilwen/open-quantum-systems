Import('env')
Import('pseudomode')
Import('protein_chain')
Import('math')
Import('OTHER_LIBS')
Import('mymode')
penv = env.Clone()
penv.Append(LIBS = [pseudomode, protein_chain, math] + OTHER_LIBS)
if mymode == 'release':
	penv.Append(CXXFLAGS = '-fwhole-program')	
pseudomode_absorption = penv.Program('pseudomode-absorption', Glob('*.cpp'))
Return('pseudomode_absorption')